<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.superclassroom">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".User"
            android:label="个人中心"/>
        <activity android:name=".Register"
            android:label="用户注册"/>
        <activity android:name=".Resetpwd"
            android:label="密码修改"></activity>
        <activity android:name=".classroomactivity"
            android:label="教室信息"></activity>
        <activity android:name=".Occupy"
            android:label="占用教室"></activity>
        <activity android:name=".UserCerter"
            android:label="个人中心"></activity>
        <activity android:name=".Release"
            android:label="解除占用"></activity>
    </application>

</manifest>